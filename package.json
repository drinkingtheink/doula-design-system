        "@fullcalendar/vue": "^4.0.2-beta",
        "axios": "^0.19",
        "popper.js": "^1.12",
        "cross-env": "^5.1",
        "jquery": "^3.2",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.4",
        "bootstrap": "4.3.1",
        "chart.js": "^2.9.3",
        "d3": "^5.7.0",
        "datamaps": "^0.5.9",
        "date-fns": "^1.30.1",
        "dropzone": "5.5.1",
        "element-ui": "^2.13.0",
