        "flatpickr": "^4.5.7",
        "fuse.js": "^3.2.0",
        "google-maps": "^3.2.1",
        "nouislider": "^12.1.0",
        "perfect-scrollbar": "^1.3.0",
        "quill": "^1.3.6",
        "sweetalert2": "^9.5.4",
        "vee-validate": "^3.2.1",
        "vue": "^2.6.11",
        "vue-chartjs": "^3.5.0",
